{% extends 'base.html.twig' %}
{% block title %}Les acteurs{% endblock %}

{% block body %}
    <h1>Tous les acteurs :</h1>

    <ul>
        {% for actor in actors %}
            <li>
                <a href="{{ path('actor_show', {id: actor.id}) }}"><h4>{{ actor.name }}</h4></a>
            </li>

        {% endfor %}
    </ul>

{% endblock %}